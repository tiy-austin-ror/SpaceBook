<p id="notice"><%= notice %></p>
<div class="container">

  <h1>Listing campuses</h1>


  <table>
    <thead>
      <tr>
        <th>Campus:</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @campus.each do |campus| %>
        <tr>
          <div>
            <p><%= campus.name %></p>
            <img src="<%= campus.picture_url %>"></img>
            <p><%= link_to 'Show', campus %></p>
          </div>
        </tr>

      <% end %>
    </tbody>
  </table>

  <br>

</div>
